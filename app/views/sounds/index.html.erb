<div class="canScroll3">
	<% @sounds.each do |s| %>
		<div class="element">
			<div class="images">
				<div class="playlist<%= s.id %>">
					<% if s.latest?  %>
						<%= image_tag(s.large_image, :alt => "#{s.title}") %>
					<% else %>
						<%= image_tag(s.image, :alt => "#{s.title}") %>
					<% end %>
				</div>
				<div class="muteEffects">
	    			<section class="hidden-infos">
						<input type="hidden" value=<%= s.id %> class="playlist-object-id"> 
						<a class="launchPlaylist"><%= link_to '', sound_path(s) %></a>
		          		<div class="overlay3"></div>
						<div class="icons4 play"></div>	        					
					</section>
		        	<div class="vidTitle">
		        		<div class="title-wrap">
		            		<h3><%= s.title %></i></h3>
		          		</div>
		        	</div>
				</div>
				<div class="muteEffects2">
	    			<section class="hidden-infos">
						<input type="hidden" value=<%= s.id %> class="playlist-object-id"> 
						<a class="launchPlaylist"></a>       					
					</section>
		        	<div class="vidTitle">
		        		<div class="titleWrap">
		            		<h3><%= s.title %></h3>
		          		</div>
		        	</div>
				</div>
	    	</div>
		</div>
	<% end %>
</div>