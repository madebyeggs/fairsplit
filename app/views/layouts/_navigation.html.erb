<div class="col-md-3">
	<div class="leftSide">
		<% if user_signed_in? %>
			<div class="userInfo">
				<div>{ Logged in as <strong><%= current_user.email %></strong> }</div>
				<%= link_to 'Edit', edit_user_registration_path %> | 
				<%= link_to 'Logout', destroy_user_session_path, :method => :delete %> |
				<%= link_to 'CMS', cms_path %></div>
			</div>
		<% end %>
		<div class="logo"><%= link_to(image_tag('splitPic.png'), news_index_path) %></div>
		<div class="navigationWrapper">
			<ul id="menu" class="navigation option-set clearfix">
				<li class="navButton"><%= link_to 'Artists', artists_path, :class => "navigationButton" %></li>
				<li class="navButton"><%= link_to 'Releases', playlists_path, :class => "navigationButton" %></li>
				<li class="navButton"><%= link_to 'Placements', works_path, :class => "navigationButton" %></li>
				<!-- <li class="navButton"><%#= link_to 'About', abouts_path, :class => "navigationButton" %></li> -->
				<li class="navButton"><%= link_to 'Contact', contacts_path, :class => "navigationButton" %></li>
			</ul>
		</div>
		<div class="social">
			<div class="centerSocial">
				<a href="https://www.facebook.com/pages/Fairsplit-Music/609198105854352" target="_blank">
					<i class="fa fa-facebook fa-lg icon"></i>
				</a>
				<a href="https://twitter.com/FairsplitMusic" target="_blank">
					<i class="fa fa-twitter fa-lg icon"></i>
				</a>
				<a href="https://instagram.com/fairsplitmusic/" target="_blank">
					<i class="fa fa-instagram fa-lg icon"></i>
				</a>
				<a href="https://www.youtube.com/channel/UCcjbocHGR4WaSlrkqSfbwBg" target="_blank">
					<i class="fa fa-youtube fa-lg icon"></i>
				</a>
				<a href="https://soundcloud.com/fairsplit-music" target="_blank">
					<i class="fa fa-soundcloud icon"></i>
				</a>
				<a class="fixMute" href="<%= contacts_path %>">
					<i class="fa fa-envelope-o fa-lg icon"></i>
				</a>
			</div>
		</div>
		<!--<div class="client"><a href="#filter=.client_login">Client Login</a></div>-->
	</div>
</div>