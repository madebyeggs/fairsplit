<div class="canScroll4 fadeMe">
	<div id="masonry-container" class="transitions-enabled infinite-scroll clearfix">
		<% @works.each do |w| %>
			<div class="box col4 wrapping">
				<div class="element">
					<div class="images">
						<div class="">
							<%= image_tag(w.grid_square_image, :alt => "#{w.client} - #{w.title}") %>
							<div class="overImage"></div>
						</div>
						<div class="muteEffects">
			    			<section class="hidden-infos">
								<a class="launchAnnouncement"><%= link_to '', placement_path(w) %></a>
				          		<div class="overlay2"></div>
								<div class="overlay3"></div>
								<div class="icons4 play"></div>	        					
							</section>
				        	<div class="vidTitle">
								<div class="title-wrap">
					            	<h3 class="descriptionSize1 upcase"><%= w.client %><i>"<%= w.title %>"</i></h3>
					          	</div>
								<div class="subtitle-wrap2">
				            		<p class="subtitle descriptionSize1 upcase italics"><%= w.artist_name %> : <%= w.track_name %></p>
								</div>
				        	</div>
						</div>
			    	</div>
				</div>
			</div>
		<% end %>
	</div>
</div>
