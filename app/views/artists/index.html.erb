<div class="canScroll4 fadeMe">
	<div id="masonry-container" class="transitions-enabled infinite-scroll clearfix">
		<% @artists.each do |a| %>
			<div class="box col4 wrapping">
				<div class="element">
					<div class="images">
						<div class="">
							<%= image_tag(a.square_image, :alt => "#{a.name}") %>
							<div class="overImage"></div>
						</div>
						<div class="muteEffects">
			    			<section class="hidden-infos">
								<a class="launchAnnouncement"><%= link_to '', artist_path(a) %></a>
				          		<div class="overlay2"></div>
								<div class="overlay3"></div>
								<div class="icons4 play"></div>	        					
							</section>
				        	<div class="vidTitle">
								<div class="title-wrap">
					            	<h3 class="descriptionSize1 upcase"><%= a.name %></h3>
					          	</div>
								<div class="subtitle-wrap2">
				            		<p class="subtitle"><%= truncate(a.description, :length => 260, :class => "descriptionSize1") %></p>
								</div>
				        	</div>
						</div>
			    	</div>
				</div>
			</div>
		<% end %>
	</div>
</div>